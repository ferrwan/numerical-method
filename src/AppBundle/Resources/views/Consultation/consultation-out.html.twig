<div id="listPayment" class="tab-pane fade in">
  {% set dates = ["10-June-2016", "13-May-2016"] %}
  {% set status = ["incomplete", "complete"] %}

  <div style="float: right">
    <button class="btn btn-default">
      <i class="fa fa-print fa-2x"></i>
    </button>
  </div>
  <h1>List Payment</h1>
  <table class="table table-striped table-hover">
    <tr>
      <th>No.</th>
      <th>Payment ID</th>
      <th>User ID</th>
      <th>Status</th>
      <th>Date</th>
    </tr>
    {% for i in 0..20  %}
    {% set label = random(status) %}
    <tr>
      <td>{{ i+1 }}</td>
      <td>{{ random(100000) }}</td>
      <td>{{ random(10000000) }}</td>
      <td><span class="label label-{{ (label == 'complete') ? 'success' : 'danger' }}">{{ (label == 'complete') ? 'Compelete' : 'Incomplete' }}</span></td>
      <td>{{ (i > 10) ? dates[1] : dates[0] }}</td>
    </tr>
    {% endfor %}
  </table>
</div>
<div id="outListPay" class="tab-pane fade in">
  {% set dates = ["10-June-2016", "13-May-2016"] %}
  {% set status = ["incomplete", "complete"] %}
  <br>
  {{ "now" | date("d/m/Y") }}
  <div style="border: 2px solid #999; padding: 8px">
    <h1>List Payment</h1>
    <table class="table table-striped table-hover">
      <tr>
        <th>No.</th>
        <th>Payment ID</th>
        <th>User ID</th>
        <th>Status</th>
        <th>Date</th>
      </tr>
      {% for i in 0..13  %}
      {% set label = random(status) %}
      <tr>
        <td>{{ i+1 }}</td>
        <td>{{ random(100000) }}</td>
        <td>{{ random(10000000) }}</td>
        <td><span class="label label-{{ (label == 'complete') ? 'success' : 'danger' }}">{{ (label == 'complete') ? 'Compelete' : 'Incomplete' }}</span></td>
        <td>{{ (i > 6) ? dates[1] : dates[0] }}</td>
      </tr>
      {% endfor %}
    </table>
  </div>
  <br>
  <div style="float: right">Page 1/1</div>
</div>
