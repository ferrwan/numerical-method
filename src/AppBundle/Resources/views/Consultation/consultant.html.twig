<div id="consultRegist" class="tab-pane fade in">
  <h1>Consultant Registration</h1>
  <div class="container">
    <form class="form-horizontal">
      <div class="form-group">
        <label class="col-xs-3 control-label">Username : </label>
        <div class="col-xs-5">
          <input class="form-control">
        </div>
      </div>
      <div class="form-group">
        <label class="col-xs-3 control-label">Name : </label>
        <div class="col-xs-5">
          <input class="form-control">
        </div>
      </div>
      <div class="form-group">
        <label class="col-xs-3 control-label">Email : </label>
        <div class="col-xs-5">
          <input class="form-control">
        </div>
      </div>
      <div class="form-group">
        <label class="col-xs-3 control-label">Password : </label>
        <div class="col-xs-5"><input class="form-control"></div>
      </div>
      <div class="form-group">
        <label class="col-xs-3 control-label">Confirm Password : </label>
        <div class="col-xs-5"><input class="form-control"></div>
      </div>
      <div class="form-group">
        <label class="col-xs-3 control-label">Birth Date : </label>
        <div class="col-xs-5"><input type="date" class="form-control"> </div>
      </div>
      <div class="form-group">
        <label class="col-xs-3 control-label">Gender : </label>
        <div class="col-xs-3">
          <input type="radio" name="gender" id="cman"> <label for="cman">Man</label>
          <input type="radio" name="gender" id="cwoman"> <label for="cwoman">Woman</label>
        </div>
      </div>
      <div class="form-group">
        <label class="col-xs-3 control-label">Phone Number : </label>
        <div class="col-xs-5"><input class="form-control"></div>
      </div>
      <div class="form-group">
        <label class="col-xs-3 control-label">Address : </label>
        <div class="col-xs-5"><input class="form-control"></div>
      </div>
      <div class="form-group">
        <div class="col-xs-2"></div>
        <button class="btn btn-success">Sign-Up</button>
      </div>
    </form>
  </div>
</div>
<div id="listPay" class="tab-pane fade in">
  <h1>Form List Payment</h1>
  <form class="form-horizontal">
    <div class="form-group">
      <label for="date" class="col-sm-3 control-label">Date : </label>
      <div class="col-sm-4"><input type="date" id="date" class="form-control"></div>
      <div class="col-sm-4"><input type="date" id="date" class="form-control"></div>
      <label for="date" class="btn btn-info" role="button"><i class="fa fa-calendar"></i></label>
    </div>
  </form>
</div>
