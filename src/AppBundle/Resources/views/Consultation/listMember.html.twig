<div id="listMember" class="tab-pane fade in">
  <div style="float: right">
    <button class="btn btn-default"><i class="fa fa-print fa-2x"></i></button>
  </div>
  <h1>List Members</h1>
  <div class="row">
  {% set class = ['danger', 'warning', 'success', 'info'] %}
  {% set gender = ['Male', 'Female'] %}
  {% for i in 0..21  %}
    <div class="col-xs-3">
      <div class="alert alert-{{ random(class) }}">
        <span><i class="fa fa-user fa-5x"></i></span>
        <div style="display: inline-block; vertical-align: top">
          <small>
            User {{ i + 1 }}<br>
            {{ random(gender) }}
          </small>
        </div>
      </div>
    </div>
    {% if (i+1) % 4 == 0 %}
    </div>
    <div class="row">
    {% endif %}
  {% endfor %}
  </div>
</div>
<div id="listConsult" class="tab-pane fade in">
  <div style="float: right">
    <button class="btn btn-default"><i class="fa fa-print fa-2x"></i></button>
  </div>
  <h1>List Consultants</h1>
  <div class="row">
  {% for i in 0..21  %}
    <div class="col-xs-3">
      <div class="alert alert-{{ random(class) }}">
        <span><img class="img-responsive" src="{{ asset('testimonial.png') }}"></span>
        <div style="display: inline-block; vertical-align: top">
          Consultant {{ i + 1 }}<br>
          {{ random(gender) }}
        </div>
      </div>
    </div>
    {% if (i+1) % 4 == 0 %}
    </div>
    <div class="row">
    {% endif %}
  {% endfor %}
  </div>
</div>
<div id="outListMember" class="tab-pane fade in">
  <br>
  {{ "now" | date("d/m/Y") }}
  <div style="border: 2px solid #999; padding: 8px">
    <h1 style="text-align: center; border-bottom: 2px solid #424242">List Members</h1>
    <h4>Total Members = 22 </h4>
    <div class="row">
    {% for i in 0..21  %}
      <div class="col-xs-3">
        <div class="alert alert-{{ random(class) }}">
          <span><i class="fa fa-user fa-4x"></i></span>
          <div style="display: inline-block; vertical-align: top">
            User {{ i + 1 }}<br>
            {{ random(gender) }}
          </div>
        </div>
      </div>
      {% if (i+1) % 4 == 0 %}
      </div>
      <div class="row">
      {% endif %}
    {% endfor %}
    </div>
  </div>
  <br>
  <div style="float: right">Page 1/1</div>
</div>
